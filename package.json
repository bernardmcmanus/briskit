{
  "name": "briskit",
  "version": "1.1.0",
  "description": "High-priority asynchronous task queue",
  "main": "dist/briskit.js",
  "config": {
    "src": "src/*.js",
    "banner": "/*! <%= pkg.name %> - <%= pkg.version %> - <%= pkg.author.name %> - <%= grunt.config.data.gitinfo.local.branch.current.shortSHA %> - <%= grunt.template.today('yyyy-mm-dd') %> */\n",
    "connect": {
      "port": 9001,
      "base": ".",
      "hostname": "0.0.0.0",
      "url": "http://<%= pkg.config.connect.hostname %>:<%= pkg.config.connect.port %>"
    }
  },
  "devDependencies": {
    "fs-extra": ">=0.9.1",
    "colors": "~1.0.3",
    "es6-promise": "~2.0.1",
    "mocha": "~1.21.4",
    "chai": "~1.9.1",
    "phantomjs": "~1.9.9",
    "grunt-mocha-phantomjs": "~1.0.0",
    "zone": "~0.3.0",
    "babelify": "~6.1.3",
    "grunt-browserify": "~3.8.0",
    "browserify-derequire": "~0.9.4",
    "grunt-wrap": "~0.3.0",
    "grunt-mocha-test": "~0.12.7",
    "grunt": "~0.4.2",
    "grunt-contrib-jshint": ">=0.10.0",
    "grunt-contrib-concat": ">=0.4.0",
    "grunt-contrib-uglify": ">=0.2.2",
    "grunt-contrib-clean": ">=0.4.1",
    "grunt-replace": ">=0.7.7",
    "grunt-gitinfo": "~0.1.7",
    "grunt-contrib-watch": ">=0.6.1",
    "grunt-import-clean": ">=0.1.0",
    "grunt-contrib-connect": "~0.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elnarddogg/briskit.git"
  },
  "keywords": [
    "event",
    "task",
    "queue",
    "async",
    "asynchronous"
  ],
  "author": {
    "name": "Bernard McManus",
    "email": "bernard.mcmanus.iv@gmail.com",
    "url": "http://github.com/elnarddogg"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/elnarddogg/briskit/blob/master/LICENSE.txt"
  },
  "bugs": {
    "url": "https://github.com/elnarddogg/briskit/issues"
  },
  "homepage": "https://github.com/elnarddogg/briskit"
}
